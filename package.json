    "egg-oss": "^2.0.0",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^5.2.1",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.2.0",
    "fs-extra": "^9.0.0",
    "highlight.js": "^10.0.3",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "markdown-it": "^10.0.0",
    "mysql2": "^2.1.0",
